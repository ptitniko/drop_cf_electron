{
    "name": "photo-electron",
    "version": "1.0.1",
    "description": "Application de communication avec la creative factory pour le traitement de photo",
    "author": "Nicolas Filloleau",
    "main": "main.js",
    "repository": {
      "type": "git",
      "url": "git@github.com:ptitniko/drop_cf_electron.git"
    },
    "scripts": {
      "start": "electron .",
      "pack": "electron-builder --dir",
      "dist": "electron-builder",
      "postinstall": "electron-builder install-app-deps"
    },
    "build": {
      "files": [
        "**/*",
        ".env"
      ],
      "appId": "com.photoelectron.app",
      "publish": [
        {
          "provider": "github",
          "owner": "ptitniko",
          "repo": "drop_cf_electron"
        }
      ],
      "mac": {
        "target": "dmg"
      },
      "win": {
        "target": "nsis"
      }
    },
    "dependencies": {
      "axios": "^1.6.0",
      "chokidar": "^3.5.3",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "electron-updater": "^6.6.2",
      "exiftool-vendored": "^23.7.0",
      "express": "^4.18.2",
      "fs-extra": "^11.1.1",
      "jimp": "^1.6.0"
    },
    "devDependencies": {
      "electron": "^29.0.0",
      "electron-builder": "^24.9.1",
      "electron-rebuild": "^3.2.9"
    }
  }